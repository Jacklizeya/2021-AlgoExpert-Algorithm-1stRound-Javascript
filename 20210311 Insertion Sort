function insertionSort(array) {
  // Write your code here.
	if (array.length <= 1) {return array}
	
	for (i=1; i< array.length; i++)
		{
			for (j = i; j >= 1; j--)
			{
				if (array[j-1] > array[j]) {let tem = array[j-1]; array[j-1] = array[j]; array[j] = tem; }
				else {break}
				console.log(i, j)			
			}
			
		}
		return array
		
		
}

// Do not edit the line below.
exports.insertionSort = insertionSort;


// good part in the front, every time add a new item 
